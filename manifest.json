{
  "manifest_version": 3,
  "name": "Github Suite",
  "description": "A suite of tools to enhance your Github experience",
  "version": "1.0",
  "options_page": "options.html",
  "permissions": ["activeTab", "storage", "tabs"],
  "host_permissions": ["https://github.com/*"],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["dist/bundle.js"]
    }
  ],
  "icons": {
    "128": "icon128.png"
  }
}
